{
  "browser_action": {
    "default_icon": "icon.png"
  },
  "description": "KissAnime adblocker",
  "name": "kStop",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.kissanime.to/*"],
      "js": ["lib/jquery.min.js", "lib/index.js"],
      "run_at": "document_start"
    }
  ]
}